{
  "name": "arcticle_ai",
  "version": "1.0.0",
  "description": "NodeJS网页版文案/文章智能生成工具",
  "main": "app.js",
  "scripts": {
    "start": "node ./app.js",
    "heart": "node ./middlewares/jobs.js",
    "heart:dev": "nodemon ./middlewares/jobs.js",
    "start:pm2": "pm2 start app.js --watch",
    "render": "export PATH=\"${PATH}:/opt/render/project/.render/chrome/opt/google/chrome\" && npm run start",
    "dev": "nodemon ./app.js",
    "renderStart": "chmod +x ./chrome.sh && bash ./chrome.sh && npm run render",
    "mkdir:dir": "mkdir -p /opt/render/project/src/chrome && chmod +x /opt/render/project/src/chrome && export PUPPETEER_DOWNLOAD_PATH='/opt/render/project/src/chrome'"
  },
  "keywords": [
    "nodejs",
    "express",
    "rest",
    "api"
  ],
  "author": "铜豌豆",
  "license": "ISC",
  "dependencies": {
    "busboy": "^1.6.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.18.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-schedule": "^2.1.1",
    "puppeteer": "^19.4",
    "serve-static": "^1.15.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.1"
  }
}
